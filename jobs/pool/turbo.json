{
  "extends": ["//"],
  "pipeline": {
    "start": {
      "dependsOn": ["build"],
      "env": ["ALCHEMY_ID", "DATABASE_URL"],
      "cache": false,
      "persistent": true
    },
    "build": {
      "outputs": [".graphclient/**"],
      "dependsOn": ["^build"]
    },
    "check": {
      "dependsOn": ["build"],
      "cache": false
    }
  }
}
